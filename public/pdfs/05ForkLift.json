            "ul": [
                {
                    "text": [
                        {
                            "text": "\"Hang the forklift\" to the center of the load and approach it in a straight direction with the tongs in the scrolling position; Stop when the tips of the tongs are about one foot from the load.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "Level the tongs and drive slowly to the front until the load is located against backside of backrest mast. ",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "Lift the load enough to clear what's underneath it.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "Step back around one foot, then gently and evenly tilt the mast back to stabilize the load.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
