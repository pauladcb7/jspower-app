            "ul": [
                {
                    "text": [
                        {
                            "text": "\"Hang the forklift\" and stop at about a foot from the desired location.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "Level the tongs and drive to the unloading site; slowly lower the load towards the floor.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "Tilt the tongs slightly forward so that you do not engage in the load.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                },
                {
                    "text": [
                        {
                            "text": "When the space behind you is free of obstructions, go back until the tongs have left the load free on the platform.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
                    ],
                    "nodeName": "LI",
                    "style": [
                        "html-li",
                        "html-ul"
                    ]
                }
            ]
