                    "text": [
                        {
                            "text": "If you discover a deteriorated portion, correct the situation if you can, or report it so that the risk be eliminated.",
                            "nodeName": "SPAN",
                            "font": "Calibri",
                            "fontSize": 12,
                            "style": [
                                "html-span",
                                "html-li",
                                "html-ul"
                            ]
                        }
